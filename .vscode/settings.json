{
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "html",
      {
        "language": "vue",
        "autoFix": true
      }
    ],
    "eslint.options": {
      "plugins": [
        "vue"
      ]
    },
    "editor.tabSize": 2,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "files.autoSave": "off",
    "git.ignoreLimitWarning": true
  }
  